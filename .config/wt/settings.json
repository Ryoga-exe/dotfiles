    "actions": [
        {
            "command": "closeWindow",
            "keys": "alt+f4"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "openNewTabDropdown",
            "keys": "ctrl+shift+space"
        },
        {
            "command": "toggleFullscreen",
            "keys": "f11"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],

